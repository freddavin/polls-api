generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Poll {
  id          String   @id @default(uuid())
  external_id Int      @default(autoincrement())
  title       String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @default(now())

  @@map("polls")
}
